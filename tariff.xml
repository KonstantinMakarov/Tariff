          xsi:schemaLocation="http://www.epam.com/Tariff tariff.xsd
http://www.epam.com/Tariff ">
    <tariff>
        <name>EasyToSay</name>
        <operatorName>MTS</operatorName>
        <payroll>50000</payroll>
        <smsPrice>500</smsPrice>
        <callPrice>
            <intraCallPrice>200</intraCallPrice>
            <externalCallPrice>800</externalCallPrice>
            <landLineCallPrice>1000</landLineCallPrice>
        </callPrice>
